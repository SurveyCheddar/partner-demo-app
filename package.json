    "remove-deps": "rm -rf node_modules && rm yarn.lock",
    "rebuild-ios": "yarn && cd ios && pod deintegrate && rm Podfile.lock && arch -x86_64 pod install --repo-update && cd ..",
    "rebuild-android": "cd android && ./gradlew clean && cd .. && rm -rf node_modules && rm yarn.lock && yarn && npx react-native run-android",
